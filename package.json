{
  "name": "@obrassard/initbot",
  "version": "1.0.0-beta.2",
  "description": "",
  "repository": "git://github.com/obrassard/initbot.git",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "main": "index.js",
  "scripts": {
    "start": "tsc && node dist/main.js",
    "dev": "node -r ts-node/register ./src/main.ts",
    "dist": "npm run clean && tsc",
    "watch": "nodemon --exec npm run dev",
    "clean": "rm -rf dist",
    "global": "npm run dist && npm install -g",
    "publish": "npm run dist && npm publish",
    "test": "mocha -t 5000 --require ts-node/register src/tests/**/*.spec.ts"
  },
  "keywords": [
    "Git",
    "CLI",
    "Initializer"
  ],
  "author": "Olivier Brassard <brassard.oli@gmail.com>",
  "license": "MIT",
  "bin": {
    "initbot": "./dist/main.js"
  },
  "dependencies": {
    "@octokit/rest": "^16.28.0",
    "chalk": "^2.3.0",
    "clear": "0.0.1",
    "clui": "^0.3.6",
    "configstore": "^3.1.1",
    "figlet": "^1.2.0",
    "inquirer": "^5.0.1",
    "lodash": "^4.17.4",
    "minimist": "^1.2.0",
    "os-locale": "^4.0.0",
    "simple-git": "^1.89.0",
    "touch": "^3.1.0"
  },
  "devDependencies": {
    "@types/clui": "^0.3.0",
    "@types/configstore": "^4.0.0",
    "@types/inquirer": "^6.5.0",
    "@types/lodash": "^4.14.138",
    "@types/node": "^12.7.5",
    "@types/touch": "^3.1.1",
    "@types/mocha": "^5.2.7",
    "eslint": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.2",
    "ts-node": "^8.4.1",
    "typescript": "^3.5.3"
  }
}
